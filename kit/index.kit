<!DOCTYPE html>
<html>
<head>
	<!-- @import "partials/head" -->
</head>
<body>
	<!-- $menuHomeActive = active -->
  	<!-- $menuProductActive = nil -->
  	<!-- $menuSupportActive = nil -->
  	<!-- $menuAboutActive = nil -->
  	<!-- HEADER -->
	<!-- @import "partials/header" -->
	<!-- @import "partials/header_home" -->
	<!-- CONTENT -->
	<div id="content">
		<div class="container">
			<div class="row">
				<div class="title">
					<h3><span>NEW</span> ARRIVALS</h3>
					<p>Lorem Ipsum is simply dummy text of the printing and typesetting industry</p>
				</div>
				<div class="products owl-carousel clearfix">
				</div>
				<div class="modal fade" id="imagemodal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
				  	<div class="modal-dialog modal-md">
				    	<div class="modal-content">              
				      		<div class="modal-body">
						      	<button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span></button>
						      	<h3 class="name"></h3>
						        <img src="" class="imagepreview">
						        <p id="price"></p>
						        <div class="col-md-4">
						        	<h5><b>Category: </b><span id="category"></span></h5>
						        </div>
						        <div class="col-md-4">
						        	<h5><b>Size: </b><span id="size"></span></h5>
						        </div>
						        <div class="col-md-4">
						        	<h5><b>Brand: </b><span id="brand"></span></h5>
						        </div>
						        <h4></h4>
						        <form>
						        	<div class="form-group">
								    	<label for="name">Name</label>
								    	<input type="text" class="form-control" id="name" placeholder="Enter Name">
								  	</div>
								  	<div class="form-group">
								    	<label for="address">Address</label>
								    	<input type="text" class="form-control" id="address" placeholder="Enter Address">
								  	</div>
								  	<div class="form-group">
								    	<label for="phone">Phone</label>
								    	<input type="text" class="form-control" id="phone" placeholder="Enter Phone">
								  	</div>
								  	<div class="form-group">
								    	<label for="email">Email address</label>
								    	<input type="name" class="form-control" id="email" aria-describedby="emailHelp" placeholder="Enter Email">
								    	<small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small>
								  	</div>
								  	<div class="form-group">
									    <label for="number">Choose number of quality</label>
									    <select class="form-control" id="number">
									      <option>1</option>
									      <option>2</option>
									      <option>3</option>
									      <option>4</option>
									      <option>5</option>
									    </select>
								 	 </div>
								        <button type="submit" class="btn btn-default"><i class="fa fa-shopping-cart" aria-hidden="true"></i> Order Now</button>
								 </form>
				      		</div>
				    	</div>
				  	</div>
				</div>
				<div class="seo col-md-12">
					<div class="row">
						<div class="col-md-7">
							<div class="sales">
								<img src="layout/images/sale.png">
								<div class="extra-seo">
									<h3>FULL WINTER KIT</h3>
									<p>Half Jacket + Skiny Trousers + Boot leather</p>
								</div>
								<span><i class="fa fa-shopping-cart" aria-hidden="true"></i><span>120$</span></span>
							</div>
						</div>
						<div class="col-md-5">
							<div class="ads">
								<img src="layout/images/ads.jpg">
							</div>
						</div>
					</div>
				</div>
				<div class="best-sales col-md-12">
					<div class="row">
						<div class="title">
							<h3><span>Best</span> sales</h3>
							<p>Lorem Ipsum is simply dummy text of the printing and typesetting industry</p>
						</div>
						<div class="products-sales clearfix">

						</div>
					</div>
				</div>
				<div class="col-md-12">
					<div class="news-letter">
						<div class="row">
							<div class="col-md-6 col-sm-12 left">
								<h3>News Letter</h3>
								<p>join us now to get all news and special offers</p>
							</div>
							<div class="col-md-6 col-sm-12 right">
								<form method="GET" class="form-horizontal" role="form">
									<input type="text" class="form-control" name="" placeholder="type your email here">
									<input type="submit" name="" value="join us">
								</form>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
	<!-- FOOTER -->
	<!-- @import "partials/footer" -->
</body>